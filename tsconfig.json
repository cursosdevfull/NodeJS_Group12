{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "ES2020",
    "moduleResolution": "node",
    "sourceMap": false,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "outDir": "./cache"
  },
  "exclude": ["node_modules", "cache"],
  "include": ["src/**/*"]
}
