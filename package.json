{
  "name": "proyecto_curso",
  "version": "1.0",
  "description": "Proyecto del curso",
  "scripts": {
    "test": "cross-env NODE_ENV=development jest --coverage --passWithNoTests",
    "test:unit": "cross-env NODE_ENV=development jest test/unit/* --coverage=true --passWithNoTests",
    "test:unique": "cross-env NODE_ENV=development jest --coverage=false  --passWithNoTests",
    "test:integration": "cross-env NODE_ENV=development jest test/integration/* --coverage=true --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=development jest --watchAll",
    "test:config": "cross-env NODE_ENV=development jest --showConfig",
    "test:clear": "cross-env NODE_ENV=development jest --clearCache",
    "test:coverage": "cross-env NODE_ENV=development jest --coverage",
    "test:coverage:unit": "npm run test:unit -- --coverage",
    "test:coverage:integration": "npm run test:integration -- --coverage",
    "lint:report": "eslint --ext ts src/ -f html -o reports/lint.html",
    "format:fix": "eslint --ext ts src --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist cache reports",
    "build": "npm run clean && tsc && gulp minify",
    "start:dev": "nodemon src/index.ts",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.341.0",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.5.1",
    "@types/uuid": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "gulp": "^4.0.2",
    "gulp-uglify-es": "^3.0.0",
    "helmet": "^6.1.5",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "jwt-simple": "^0.5.6",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.3.0",
    "neverthrow": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "typeorm": "^0.3.15",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^18.15.9",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "jest-html-reporter": "^3.10.0",
    "jest-html-reporters": "^3.1.4",
    "jest-sonar-reporter": "^2.0.0",
    "node-mocks-http": "^1.12.2",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  }
}
